@model FaturaYonetimSistemi.Shared.Dtos.KullaniciAidatDto.InsertKullaniciAidatDto


<div class="col-md-12">
    <div asp-validation-summary="ModelOnly"></div>
    <form class="w-50" asp-action="Add" asp-controller="KullaniciAidat" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label>Kullanıcı Id</label>
            <span asp-validation-for="KullaniciId" class="text-danger"></span>
            <input type="text" class="form-control" value="" asp-for="KullaniciId" />
        </div>
        <div class="form-group">
            <label>Aidat Id</label>
            <span asp-validation-for="AidatId" class="text-danger"></span>
            <input type="text" class="form-control" value="" asp-for="AidatId" />
        </div>
        <div class="form-check">
            <input type="checkbox" asp-for="OdendiMi" class="form-check-input" />
            <label class="form-check-label">Ödendi Mi?</label>
        </div>
        <br />
        <div class="form-group">
            <label>Ödenme Tarihi</label>
            <input type="date" class="form-control" asp-for="OdenmeTarihi" />
        </div>
        <div class="form-check">
            <input type="checkbox" asp-for="AktifMi" class="form-check-input" />
            <label class="form-check-label">Aktif Mi?</label>
        </div>
        <br /><br />
        
        <div class="form-group">
            <button class="btn btn-success" type="submit">Ekle</button>
            <button class="btn btn-warning" type="reset">Temizle</button>
            <a class="btn btn-info" asp-action="Index" asp-controller="Home" asp-area="Yonetici">Geri</a>
        </div>
    </form>
</div>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>