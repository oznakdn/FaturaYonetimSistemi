@model List<FaturaYonetimSistemi.Shared.Dtos.KullaniciAidatDto.ListKullaniciAidatDto>



<table class="table table-hover table-striped">
    <thead class="thead-light">
        <tr>
            <th>Dönem</th>
            <th>Tutar</th>
            <th>Son Ödeme Tarihi</th>
            <th>Ödendi Mi?</th>
            <th>Ödenme Tarihi</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>


                <td>@item.Donem</td>
                <td>@item.Tutar</td>
                <td>@item.SonÖdemeTarihi</td>
                <td>@item.OdendiMi</td>
                <td>@item.OdenmeTarihi</td>
                <td><a class="btn btn-warning" asp-action="Update" asp-controller="KullaniciAidat" asp-route-id="@item.Id">Güncelle</a></td>
                <td><a class="btn btn-danger" asp-action="Delete" asp-controller="KullaniciAidat" asp-route-id="@item.Id">Sil</a></td>
            </tr>
        }
    </tbody>
</table>

<a class="btn btn-info" asp-action="GetAll" asp-controller="Kullanici">Geri</a>
